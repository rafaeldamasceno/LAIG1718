<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="20" />
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
</ILLUMINATION>


<!-- Lights -->
<LIGHTS>
	<LIGHT id="light0">
        <enable value ="1" />
        <position  x="0.1" y="16.5" z="0" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

	<LIGHT id="light1">
        <enable value ="1" />
        <position x="59.1" y="16.5" z="0" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

	<LIGHT id="light2">
        <enable value ="1" />
        <position x="0.1" y="16.5" z="40" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
        <enable value ="1" />
        <position x="59.1" y="16.5" z="40" w="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
        <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>

</LIGHTS>

<!-- Textures -->
<TEXTURES>
	<!-- Crowd -->
    <TEXTURE id="crowdTexture">
        <file path="images/crowd-peeps.jpg" />
        <amplif_factor s="60" t="30" />
    </TEXTURE>

     <!-- Wall -->
    <TEXTURE id="wallTexture">
        <file path="images/wall.png" />
        <amplif_factor s="1" t="5" />
    </TEXTURE>

    <!-- Rocks -->
    <TEXTURE id="rocks">
        <file path="images/rocks.jpg" />
        <amplif_factor s="2" t="2" />
    </TEXTURE>

    <!-- Arena -->
    <TEXTURE id="arenaTexture">
        <file path="images/arena.png" />
        <amplif_factor s="18" t="16" />
    </TEXTURE>

    <!-- Gym Walls -->
    <TEXTURE id="gymTexture">
        <file path="images/gym.jpg" />
        <amplif_factor s="93.56152" t="16.5" />
    </TEXTURE>

    <!-- Metalic -->
    <TEXTURE id="metalic">
        <file path="images/metalic.jpg" />
        <amplif_factor s="93.56152" t="50" />
    </TEXTURE>

    <!-- Stone -->
    <TEXTURE id="stone">
        <file path="images/outsideWall.jpg" />
        <amplif_factor s="8" t="5.5" />
    </TEXTURE>

</TEXTURES>


<MATERIALS>
	<!-- Dark material -->
	<MATERIAL id="pitchBlack">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.05" g="0.05" b="0.05" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

	<!-- Pokeball Red material -->
	<MATERIAL id="pokebalRed">
        <shininess value="1" />
        <specular r="0.23" g="0.02" b="0.02" a="1" />
        <diffuse r="0.46" g="0.04" b="0.04" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Arena material -->
    <MATERIAL id="arenaMaterial">
        <shininess value="100" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.0" g="0.0" b="0.0" a="1" />
        <ambient r="0.0742" g="0.293" b="0.3477" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Light metal material -->
    <MATERIAL id="lightMaterial">
        <shininess value="100" />
        <specular r="0.8" g="0.8" b="0.8" a="1" />
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />
        <ambient r="0.4" g="0.4" b="0.4" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

</MATERIALS>

<ANIMATIONS>

  <ANIMATION id="linear" speed="10" type="linear">
    <controlpoint xx="0" yy="0" zz="0" />
    <controlpoint xx="0" yy="16.8" zz="16.8" />
    <controlpoint xx="16.8" yy="16.8" zz="16.8" />
    <controlpoint xx="16.8" yy="16.8" zz="0" />
    <controlpoint xx="0" yy="0" zz="0" />
  </ANIMATION>

  <ANIMATION id="open" speed="100" type="linear">
    <controlpoint xx="0" yy="0" zz="0" />
    <controlpoint xx="0" yy="20" zz="0" />
    <controlpoint xx="0" yy="0" zz="0" />
  </ANIMATION>

  <ANIMATION id="circular" speed="10" type="circular"
    centerx="5" centery="0" centerz="0"
    radius="5" startang="180" rotang="360" />

  <ANIMATION id="bezier" speed="20" type="bezier">
    <controlpoint xx="0" yy="0" zz="0" />
    <controlpoint xx="20" yy="0" zz="50" />
    <controlpoint xx="-20" yy="0" zz="50" />
    <controlpoint xx="0" yy="0" zz="0" />
  </ANIMATION>
  <ANIMATION id="combo" type="combo">
    <SPANREF id="linear" />
    <SPANREF id="circular" />
    <SPANREF id="bezier" />
    <!-- ... -->
  </ANIMATION>
</ANIMATIONS>

<!-- The leaves -->
<LEAVES>
</LEAVES>

<!-- The nodes -->
<NODES>

    <ROOT id="root" />

    <!-- The root node -->
    <NODE id="root">
        <MATERIAL id="defaultMaterial" />
        <TEXTURE id="null" />

        <DESCENDANTS>
            <NODEREF id="pokeballs" />
            <NODEREF id="stairs" />
            <NODEREF id="floor" />
            <NODEREF id="stage" />
            <NODEREF id="viewers" />
            <NODEREF id="walls"/>
            <!-- <NODEREF id="roof"/> -->
        </DESCENDANTS>

    </NODE>

    <!-- The floor -->
    <NODE id="floor">
        <MATERIAL id="null" />
        <TEXTURE id="rocks" />

        <SCALE sx="12" sy="2" sz="10" />
        <TRANSLATION x="0" y="0" z="4" />
        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
			<LEAF  type="rectangle" args="0 4 5 0" />
		</DESCENDANTS>

    </NODE>

    <!-- The Viewers -->
    <NODE id="viewers">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
			<NODEREF id="viewers1"/>
            <NODEREF id="viewers2"/>
		</DESCENDANTS>

    </NODE>

    <!-- The first Viewers -->
    <NODE id="viewers1">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
            <NODEREF id="rails" />
			<NODEREF id="ramp"/>
            <NODEREF id="leftTriangle"/>
            <NODEREF id="rightTriangle"/>
            <NODEREF id="leftRectangle"/>
            <NODEREF id="rightRectangle"/>
            <NODEREF id="backRectangle"/>
		</DESCENDANTS>

    </NODE>

    <!-- The second Viewers -->
    <NODE id="viewers2">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="60" y="0" z="40"/>
        <ROTATION axis="y" angle="180"/>

        <DESCENDANTS>
            <NODEREF id="rails" />
			<NODEREF id="ramp"/>
            <NODEREF id="leftTriangle"/>
            <NODEREF id="rightTriangle"/>
            <NODEREF id="leftRectangle"/>
            <NODEREF id="rightRectangle"/>
            <NODEREF id="backRectangle"/>
		</DESCENDANTS>

    </NODE>

    <!-- The Viewers -->
    <NODE id="ramp">
        <MATERIAL id="null" />
        <TEXTURE id="crowdTexture" />

        <TRANSLATION x="0" y="1.5" z="-0.8" />
        <ROTATION axis="x" angle="-60"/>

        <DESCENDANTS>
			<LEAF type="rectangle" args="0 30 60 0" />
		</DESCENDANTS>

    </NODE>

    <!-- The left Triangle -->
    <NODE id="leftTriangle">
        <MATERIAL id="null" />
        <TEXTURE id="stone" />

        <DESCENDANTS>
			<LEAF type="triangle" args="0 16.5 -26.78076 0 1.5 -26.78076 0 1.5 -0.8" />
		</DESCENDANTS>

    </NODE>

    <!-- The left Rectangle -->
    <NODE id="leftRectangle">
        <MATERIAL id="null" />
        <TEXTURE id="stone" />

        <TRANSLATION x="0" y="0" z="-0.8"/>
        <ROTATION axis="y" angle="90"/>

        <DESCENDANTS>
			<LEAF type="rectangle" args="0 0 25.98076 1.5" />
		</DESCENDANTS>

    </NODE>

    <!-- The left Triangle -->
    <NODE id="rightTriangle">
        <MATERIAL id="null" />
        <TEXTURE id="stone" />

        <DESCENDANTS>
			<LEAF type="triangle" args="60 16.5 -26.78076 60 1.5 -0.8 60 1.5 -26.78076" />
		</DESCENDANTS>

    </NODE>

    <!-- The right Rectangle -->
    <NODE id="rightRectangle">
        <MATERIAL id="null" />
        <TEXTURE id="stone" />

        <TRANSLATION x="60" y="0" z="-26.78076"/>
        <ROTATION axis="y" angle="-90"/>

        <DESCENDANTS>
			<LEAF type="rectangle" args="0 0 25.98076 1.5" />
		</DESCENDANTS>

    </NODE>

    <!-- The back Rectangle -->
    <NODE id="backRectangle">
        <MATERIAL id="null" />
        <TEXTURE id="stone" />

        <TRANSLATION x="0" y="0" z="-26.78076"/>

        <DESCENDANTS>
			<LEAF type="rectangle" args="0 0 60 16.5" />
		</DESCENDANTS>

    </NODE>

    <!-- The Pokeball -->
     <NODE id="pokeball">
        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <ROTATION axis="y" angle="90"/>

        <DESCENDANTS>
            <NODEREF id="top_disc" />
            <NODEREF id="bot_disc" />
            <NODEREF id="center_disc" />
        </DESCENDANTS>

    </NODE>

    <!-- The Pokeballs -->
    <NODE id="pokeballs">
       <MATERIAL id="null" />
       <TEXTURE id="null" />


       <DESCENDANTS>
           <NODEREF id="pokeball1" />
           <NODEREF id="pokeball2" />
       </DESCENDANTS>

   </NODE>

    <!-- The first Pokeball -->
     <NODE id="pokeball1" selectable="true">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="11" y="5" z="20" />
        <ROTATION axis="y" angle="90"/>

        <ANIMATIONREFS>
            <!-- <ANIMATIONREF id="linear" />
            <ANIMATIONREF id="circular" /> -->
            <ANIMATIONREF id="combo" />
        </ANIMATIONREFS>

        <DESCENDANTS>
            <NODEREF id="pokeball" />
        </DESCENDANTS>

    </NODE>

    <!-- The second Pokeball -->
     <NODE id="pokeball2" selectable="true">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="49" y="5" z="20" />
        <ROTATION axis="y" angle="-90"/>

        <ANIMATIONREFS>
            <!-- <ANIMATIONREF id="circular" />
            <ANIMATIONREF id="bezier" /> -->
            <ANIMATIONREF id="combo" />
        </ANIMATIONREFS>

        <DESCENDANTS>
            <NODEREF id="pokeball" />
        </DESCENDANTS>

    </NODE>

    <!-- The Top Disc -->
    <NODE id="top_disc">
        <MATERIAL id="pokebalRed" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0.1" z="0" />
        <!-- <SCALE sx="-1" sy="1" sz="1" /> -->

        <DESCENDANTS>
						<LEAF id="sphere1" type="sphere" args="2 40 20" />
        </DESCENDANTS>

    </NODE>

    <!-- The Bot Disc -->
    <NODE id="bot_disc">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <!-- <SCALE sx="-1" sy="1" sz="1" /> -->

        <DESCENDANTS>
						<LEAF id="sphere1" type="sphere" args="2 40 20" />
        </DESCENDANTS>

    </NODE>

    <!-- The Center Disc -->
    <NODE id="center_disc">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <!-- <TRANSLATION x="0" y="10" z="0" />
        <SCALE sx="-1" sy="1" sz="1" /> -->

        <DESCENDANTS>
            <NODEREF id="horizontal_cylinder"/>
            <NODEREF id="button_sphere"/>
            <NODEREF id="button_cylinder"/>
        </DESCENDANTS>

    </NODE>

    <!-- The Horizontal Cylinder -->
    <NODE id="horizontal_cylinder">
        <MATERIAL id="pitchBlack" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="-0.05" z="0" />
        <SCALE sx="1.0031" sy="0.05" sz="1.0031" />
        <ROTATION axis="x" angle="-90"/>

        <DESCENDANTS>
            <LEAF type="cylinder" args="4 2 2 10 40 1 1"/>
        </DESCENDANTS>

    </NODE>

    <!-- The Button Sphere -->
    <NODE id="button_sphere">
        <MATERIAL id="pitchBlack" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="1.75" />

        <DESCENDANTS>
            <LEAF type="sphere" args="0.4 40 20"/>
        </DESCENDANTS>

    </NODE>

    <!-- The Button Cylinder -->
    <NODE id="button_cylinder">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.65" />
        <SCALE sx="1" sy="1" sz="1" />

        <DESCENDANTS>
            <LEAF type="cylinder" args="1.5 0.2 0.2 40 20 1 1"/>
        </DESCENDANTS>

    </NODE>

    <!-- The Stairs -->
    <NODE id="stairs">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <DESCENDANTS>
           <NODEREF id="stair1" />
           <NODEREF id="stair2" />
       </DESCENDANTS>

   </NODE>

   <!-- The Stair -->
  <NODE id="stair">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="-3" y="0" z="-3"/>

       <DESCENDANTS>
           <NODEREF id="bot_step" />
           <NODEREF id="mid_step" />
           <NODEREF id="top_step" />
       </DESCENDANTS>

   </NODE>

   <!-- The Bottom Step -->
   <NODE id="bot_step">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="3" y="0.5" z="0"/>
       <SCALE sx="6" sy="1" sz="1"/>
       <ROTATION axis="z" angle="45"/>

       <DESCENDANTS>
           <LEAF type="cylinder" args="6 0.7071068 0.7071068 20 4 1 1" />
       </DESCENDANTS>

   </NODE>

   <!-- The Middle Step -->
   <NODE id="mid_step">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="4" y="1.5" z="0"/>
       <SCALE sx="4" sy="1" sz="1"/>
       <ROTATION axis="z" angle="45"/>

       <DESCENDANTS>
           <LEAF type="cylinder" args="6 0.7071068 0.7071068 20 4 1 1" />
       </DESCENDANTS>

   </NODE>

   <!-- The Top Step -->
   <NODE id="top_step">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="5" y="2.5" z="0"/>
       <SCALE sx="2" sy="1" sz="1"/>
       <ROTATION axis="z" angle="45"/>

       <DESCENDANTS>
           <LEAF type="cylinder" args="6 0.7071068 0.7071068 20 4 1 1" />
       </DESCENDANTS>

   </NODE>

   <!-- The first Stairs -->
   <NODE id="stair1">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="9" y="0" z="20"/>

       <DESCENDANTS>
           <NODEREF id="stair" />
       </DESCENDANTS>

   </NODE>

   <!-- The second Stairs -->
   <NODE id="stair2">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="51" y="0" z="20"/>
       <ROTATION axis="y" angle="180"/>

       <DESCENDANTS>
           <NODEREF id="stair" />
       </DESCENDANTS>

   </NODE>

   <!-- The Stage -->
   <NODE id="stage" selectable="true">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="12" y="0" z="9.5"/>
       <SCALE sx="2" sy="1" sz="1.3"/>

       <DESCENDANTS>
           <NODEREF id="frontStage" />
           <NODEREF id="upStage" />
           <NODEREF id="leftStage" />
           <NODEREF id="rightStage" />
           <NODEREF id="backStage" />
       </DESCENDANTS>

   </NODE>

   <!-- The Front of the stage -->
   <NODE id="frontStage">
       <MATERIAL id="arenaMaterial" />
       <TEXTURE id="null" />

       <TRANSLATION x="0" y="3" z="16"/>
       <ROTATION axis="x" angle="180"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 18 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The up side of the stage -->
   <NODE id="upStage">
       <MATERIAL id="null" />
       <TEXTURE id="arenaTexture" />

       <TRANSLATION x="0" y="3" z="0"/>
       <ROTATION axis="x" angle="90"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 18 16" />
       </DESCENDANTS>

   </NODE>

   <!-- The Back of the stage -->
   <NODE id="backStage">
       <MATERIAL id="arenaMaterial" />
       <TEXTURE id="null" />

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 18 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The left side of the stage -->
   <NODE id="leftStage">
       <MATERIAL id="arenaMaterial" />
       <TEXTURE id="null" />

       <TRANSLATION x="0" y="0" z="16"/>
       <ROTATION axis="y" angle="90"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 16 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The right side of the stage -->
   <NODE id="rightStage">
       <MATERIAL id="arenaMaterial" />
       <TEXTURE id="null" />

       <TRANSLATION x="18" y="0" z="0"/>
       <ROTATION axis="y" angle="-90"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 16 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The Wall -->
   <NODE id="wall">
       <MATERIAL id="null" />
       <TEXTURE id="wallTexture" />

       <TRANSLATION x="0" y="0" z="-0.8"/>
       <SCALE sx="3.3333333333333333333333333" sy="1" sz="0.05"/>

       <DESCENDANTS>
           <NODEREF id="frontWall" />
           <NODEREF id="upWall" />
           <NODEREF id="leftWall" />
           <NODEREF id="rightWall" />
           <NODEREF id="backWall" />
       </DESCENDANTS>

   </NODE>

   <!-- The Front of the wall -->
   <NODE id="frontWall">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="0" y="3" z="16"/>
       <ROTATION axis="x" angle="180"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 18 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The up side of the wall -->
   <NODE id="upWall">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="0" y="3" z="0"/>
       <ROTATION axis="x" angle="90"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 18 16" />
       </DESCENDANTS>

   </NODE>

   <!-- The Back of the wall -->
   <NODE id="backWall">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 18 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The left side of the wall -->
   <NODE id="leftWall">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="0" y="0" z="16"/>
       <ROTATION axis="y" angle="90"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 16 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The right side of the wall -->
   <NODE id="rightWall">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="18" y="0" z="0"/>
       <ROTATION axis="y" angle="-90"/>

       <DESCENDANTS>
           <LEAF  type="rectangle" args="0 0 16 3" />
       </DESCENDANTS>

   </NODE>

   <!-- The rails -->
   <NODE id="rails">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <DESCENDANTS>
           <NODEREF id="railing"/>
           <NODEREF id="wall"/>
        </DESCENDANTS>

   </NODE>

   <!-- The Railing -->
   <NODE id="railing">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <DESCENDANTS>
           <NODEREF id="railing1" />
           <NODEREF id="railing2" />
           <NODEREF id="railing3" />
           <NODEREF id="railing4" />
           <NODEREF id="railing5" />
           <NODEREF id="railing6" />
           <NODEREF id="railing7" />
           <NODEREF id="railing8" />
           <NODEREF id="railing9" />
           <NODEREF id="railing10" />
       </DESCENDANTS>

   </NODE>

   <!-- The first Railing -->
   <NODE id="railing1">
       <MATERIAL id="null" />
       <TEXTURE id="null" />

       <TRANSLATION x="3" y="3" z="-0.4"/>

       <DESCENDANTS>
           <NODEREF id="rail" />
       </DESCENDANTS>

