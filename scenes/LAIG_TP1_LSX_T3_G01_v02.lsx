    <ANIMATION id="earthquake" speed="1" type="linear">
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="0.5" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="0.5" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="0.5" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="0.5" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="0.5" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="0.5" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="0.5" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
    </ANIMATION>

    <ANIMATION id="liftoff" speed="4" type="linear">
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="0" yy="8" zz="0" />
      <controlpoint xx="0" yy="0" zz="0" />
    </ANIMATION>
    <ANIMATION id="landing" speed="4" type="linear">
      <controlpoint xx="0" yy="5" zz="0" />
      <controlpoint xx="0" yy="5" zz="-3" />
      <controlpoint xx="0" yy="0" zz="0" />
    </ANIMATION>

    <ANIMATION id="roudabout" speed="4" type="circular"
      centerx="0" centery="5" centerz="0"
      radius="5" startang="0" rotang="360" />

    <ANIMATION id="diveup" speed="4" type="bezier">
      <controlpoint xx="0" yy="5" zz="0" />
      <controlpoint xx="0" yy="7" zz="5" />
      <controlpoint xx="0" yy="9" zz="5" />
      <controlpoint xx="0" yy="10" zz="0" />
    </ANIMATION>
    <ANIMATION id="divedown" speed="4" type="bezier">
      <controlpoint xx="0" yy="10" zz="0" />
      <controlpoint xx="0" yy="9" zz="-5" />
      <controlpoint xx="0" yy="7" zz="-5" />
      <controlpoint xx="0" yy="5" zz="0" />
    </ANIMATION>
    <ANIMATION id="monumentAnimation" type="combo">
      <SPANREF id="liftoff" />
      <SPANREF id="roudabout" />
      <SPANREF id="diveup" />
      <SPANREF id="divedown" />
      <SPANREF id="landing" />
    <!-- ... -->
    </ANIMATION>
  </ANIMATIONS>

