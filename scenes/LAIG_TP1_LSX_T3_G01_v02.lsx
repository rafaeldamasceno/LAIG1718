    <MATERIAL id="GrassMaterial">
      <shininess value="1" />
      <specular r="0.2" g="0.2" b="0.2" a="1" />
      <diffuse r="0.7109375" g="0.8515625" b="0.28515625" a="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
    <!-- Matte material -->
    <MATERIAL id="MatteMaterial">
      <shininess value="1000" />
      <specular r="0.91015625" g="0.92578125" b="0.83984375" a="1" />
      <diffuse r="0.7" g="0.7" b="0.7" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
    <!-- Metal material -->
    <MATERIAL id="MetalMaterial">
      <shininess value="6" />
      <specular r="0.9" g="0.9" b="0.9" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.3" g="0.3" b="0.3" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
  </MATERIALS>

