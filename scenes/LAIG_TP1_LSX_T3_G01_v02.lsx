<SCENE>
  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500" />
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
  </INITIALS>
  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0.52" g="0.8" b="0.917" a="1" />
  </ILLUMINATION>
  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light0">
      <enable value="1" />
      <position x="0" y="10" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>
    <LIGHT id="light1">
      <enable value="1" />
      <position x="24" y="10" z="0" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>
    <LIGHT id="light2">
      <enable value="1" />
      <position x="0" y="10" z="24" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>
    <LIGHT id="light3">
      <enable value="1" />
      <position x="24" y="10" z="24" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
      <specular r="0.6" g="0.6" b="0.6" a="1.0" />
    </LIGHT>
  </LIGHTS>
  <!-- Textures -->
  <TEXTURES>
    <!-- Floor Grass -->
    <TEXTURE id="grass">
      <file path="images/grass.jpg" />
      <amplif_factor s="12" t="12" />
    </TEXTURE>
    <!-- Inoxidable metal -->
    <TEXTURE id="inox">
      <file path="images/inox.jpg" />
      <amplif_factor s="5" t="5" />
    </TEXTURE>
  </TEXTURES>
  <MATERIALS>
